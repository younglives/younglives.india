<?xml version="1.0"?>
<portlets
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plone">

 <assignment
     purge="True"
     manager="plone.rightcolumn"
     category="context"
     key="/"
     />

 <assignment
     manager="plone.rightcolumn"
     category="context"
     key="/"
     type="younglives.portlets.Latest"
     name="LatestPortlet">
     <property name="limit">5</property>
 </assignment>

 <assignment
     insert-before="*"
     manager="plone.leftcolumn"
     category="context"
     key="/"
     type="younglives.india.portlets.logo"
     name="IndiaLogo"
     />

</portlets>
